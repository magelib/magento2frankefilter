<?xml version="1.0"?> 
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="OM_Productregistration::system_config" title="Productregistration" />
                        </resource>
                    </resource>
                </resource>
				<resource id="OM_Productregistration::productregistration" title="Productregistration" sortOrder="100">
                    <resource id="OM_Productregistration::add_productregistration" title="Add productregistration" sortOrder="1" />
                    <resource id="OM_Productregistration::manage_productregistration" title="Manage productregistration"  sortOrder="2" />
                    <resource id="OM_Productregistration::group_productregistration" title="Productregistration Group"  sortOrder="3" />
                    <resource id="OM_Productregistration::configuration" title="Configurations"  sortOrder="4" />
                </resource>               
            </resource>
        </resources>
    </acl>	   	
</config>