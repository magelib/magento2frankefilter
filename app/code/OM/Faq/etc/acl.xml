<?xml version="1.0"?> 
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="OM_Faq::system_config" title="Faq" />
                        </resource>
                    </resource>
                </resource>
				<resource id="OM_Faq::faq" title="Faq" sortOrder="100">
                    <resource id="OM_Faq::add_faq" title="Add faq" sortOrder="1" />
                    <resource id="OM_Faq::manage_faq" title="Manage faq"  sortOrder="2" />
                    <resource id="OM_Faq::group_faq" title="Faq Group"  sortOrder="3" />
                    <resource id="OM_Faq::configuration" title="Configurations"  sortOrder="4" />
                </resource>               
            </resource>
        </resources>
    </acl>	   	
</config>